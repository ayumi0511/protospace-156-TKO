<div class="card">
  <%= link_to image_tag(prototype_image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype_title, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype_catch_copy %>
    </p>
    <%= link_to current_user.nickname, root_path, class: :card__user %>
  </div>
</div>
